<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <UseWPF>true</UseWPF>
    <Platforms>AnyCPU;x86</Platforms>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="LiveCharts" Version="0.9.7" />
    <PackageReference Include="LiveCharts.Wpf" Version="0.9.7" />
    <PackageReference Include="Newtonsoft.Json" Version="12.0.3" />
    <PackageReference Include="NLog" Version="4.7.13" />
    <PackageReference Include="PropertyChanged.Fody" Version="3.2.8" />
    <PackageReference Include="System.IO.Ports" Version="6.0.0" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="Components\LineSeriesCursor.xaml.cs">
      <SubType>Code</SubType>
    </Compile>
    <Compile Update="Pages\CalculationRth.xaml.cs">
      <SubType>Code</SubType>
    </Compile>
    <Compile Update="Pages\GraduationCalculation.xaml.cs">
      <SubType>Code</SubType>
    </Compile>
    <Compile Update="Pages\CalculationZth.xaml.cs">
      <SubType>Code</SubType>
    </Compile>
    <Compile Update="Pages\GraduationOnly.xaml.cs">
      <SubType>Code</SubType>
    </Compile>
    <Compile Update="Pages\ZthPulseSequence.xaml.cs">
      <SubType>Code</SubType>
    </Compile>
    <Compile Update="Properties\Resource.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>Resource.resx</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Update="Properties\Resource.resx">
      <Generator>PublicResXFileCodeGenerator</Generator>
      <LastGenOutput>Resource.Designer.cs</LastGenOutput>
    </EmbeddedResource>
  </ItemGroup>

  <ItemGroup>
    <None Update="appsetting.json">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
    <None Update="Dataset.csv">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
    <None Update="NLog.config">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
  </ItemGroup>

  <ItemGroup>
    <Page Update="Components\LineSeriesCursor.xaml">
      <SubType>Designer</SubType>
    </Page>
    <Page Update="Pages\CalculationRth.xaml">
      <SubType>Designer</SubType>
    </Page>
    <Page Update="Pages\GraduationCalculation.xaml">
      <SubType>Designer</SubType>
    </Page>
    <Page Update="Pages\CalculationZth.xaml">
      <SubType>Designer</SubType>
    </Page>
    <Page Update="Pages\GraduationOnly.xaml">
      <SubType>Designer</SubType>
    </Page>
    <Page Update="Pages\ZthPulseSequence.xaml">
      <SubType>Designer</SubType>
    </Page>
    <Page Update="Resources\Chart.xaml">
      <SubType>Designer</SubType>
    </Page>
    <Page Update="Resources\CheckBox.xaml">
      <SubType>Designer</SubType>
    </Page>
    <Page Update="Resources\Consts.xaml">
      <SubType>Designer</SubType>
    </Page>
    <Page Update="Resources\ComboBox.xaml">
      <SubType>Designer</SubType>
    </Page>
    <Page Update="Resources\Button.xaml">
      <SubType>Designer</SubType>
    </Page>
    <Page Update="Resources\BottomPanelValues.xaml">
      <SubType>Designer</SubType>
    </Page>
    <Page Update="Resources\RightPanelStyles.xaml">
      <SubType>Designer</SubType>
    </Page>
    <Page Update="Resources\TextBox.xaml">
      <SubType>Designer</SubType>
    </Page>
  </ItemGroup>

  <ItemGroup>
    <Folder Include="Lib\" />
  </ItemGroup>

  <ItemGroup>
    <Reference Include="SCCI_IO">
      <HintPath>Lib\SCCI_IO.dll</HintPath>
    </Reference>
  </ItemGroup>

</Project>